<div class="card">
    <div class="card-body">
        <h5 class="card-title">
            <%= project.title %> 
        </h5>
        <p class="card-text">
            <%= project.description %> 
        </p>
        <% if (project.projectUrl){ %>
            <a href="<%= project.projectUrl %>" class="btn btn-primary">Link to Project</a>
            <% } %> 
    </div>
</div>