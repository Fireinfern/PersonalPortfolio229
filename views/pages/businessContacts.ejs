<!DOCTYPE html>
<html lang="en">

<head>
    <%- include("../partials/head", {title: pagesData[currentPage].displayName }) %>
</head>

<body>
    <!-- Header -->
    <%- include("../partials/header", { pagesData: pagesData }) %>
        <!-- Grid Display -->
        <div class="container-fluid d-flex flex-column justify-content-center align-items-center" full-height>
            <table class="table table-dark table-striped">
                <thead>
                    <tr>
                        <th scope="col">#</th>
                        <th scope="col">Name</th>
                        <th scope="col">Contact Number</th>
                        <th scope="col">Email</th>
                        <th scope="col">Actions</th>
                    </tr>
                </thead>
                <tbody>
                    <% for(let i=0; i < contacts.length; i++){ %>
                        <%- include("../partials/contactRow", {id: i + 1, contact: contacts[i]}) %>
                            <% } %>
                </tbody>
            </table>
        </div>
        <!-- Footer --->
        <%- include("../partials/footer", {pagesData: pagesData}) %>
</body>

</html>