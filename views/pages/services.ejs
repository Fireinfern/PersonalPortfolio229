<!DOCTYPE html>
<html lang="en">

<head>
    <%- include("../partials/head", {title: pagesData[currentPage].displayName }) %>
</head>

<body>
    <!-- Header --->
    <%- include("../partials/header", { pagesData: pagesData }) %>
        <div class="container" full-height>
            <h3>Services</h3>
            <div class="row">
                <% for (const service in servicesData){ %>
                    <div class="col-lg-4 col-sm-12">
                        <%- include('../partials/serviceCard', {service: servicesData[service]}) %>
                    </div>
                    <% } %>
            </div>
        </div>
        <!-- Footer --->
        <%- include("../partials/footer", {pagesData: pagesData}) %> 
</body>

</html>