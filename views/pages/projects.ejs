<!DOCTYPE html>
<html lang="en">

<head>
    <%- include("../partials/head", {title: pagesData[currentPage].displayName }) %>
</head>

<body>
    <!-- Header --->
    <%- include("../partials/header", { pagesData: pagesData }) %>
    <!--* Projects --->
    <div class="container" full-height>
        <div class="row">
            <% projectsData.forEach(project => { %>
                <div class="col-lg-6 col-sm-12">
                    <%- include("../partials/projectCard", {project: project}) %> 
                </div>
            <% }); %> 
        </div>
    </div>
    <!-- Footer --->
    <%- include("../partials/footer", {pagesData: pagesData}) %> 
</body>

</html>